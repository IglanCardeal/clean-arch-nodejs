{
  "name": "nodejs-tdd-ddd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/main/server.ts",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --passWithNoTests --silent",
    "test:verbose": "jest --passWithNoTests --verbose --watch",
    "test:v": "yarn test:verbose",
    "test:unit": "yarn test",
    "test:unit:watch": "yarn test --watch",
    "test:u": "yarn test:unit",
    "test:u:w": "yarn test:unit:watch",
    "test:integration": "yarn test -c jest-integration.config.js --runInBand",
    "test:integration:watch": "yarn test -c jest-integration.config.js --watch --runInBand",
    "test:integration:verbose": "yarn jest -c jest-integration.config.js --runInBand",
    "test:i": "yarn test:integration",
    "test:i:w": "yarn test:integration:watch",
    "test:i:v": "yarn test:integration:verbose",
    "test:staged": "yarn test:unit",
    "test:coverage": "yarn test --coverage",
    "test:integration:coverage": "yarn test:integration --coverage",
    "test:ci": "yarn test:coverage --ci --updateSnapshot && yarn test:integration:coverage --ci --updateSnapshot",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.9.0",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "*"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "fast-glob": "^3.2.7",
    "mongodb": "^4.1.1",
    "validator": "^13.6.0"
  }
}
