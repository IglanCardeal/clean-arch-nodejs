{
  "name": "nodejs-tdd-ddd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsnd -r tsconfig-paths/register --respawn --ignore-watch node_modules src/main/server.ts",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --passWithNoTests --silent",
    "test:verbose": "jest --passWithNoTests --verbose",
    "test:unit": "yarn test",
    "test:unit:watch": "yarn test --onlyChanged --watch",
    "test:u": "yarn test:unit",
    "test:u:w": "yarn test:unit:watch",
    "test:integration": "yarn test -c jest-integration.config.js --runInBand",
    "test:integration:verbose": "jest --passWithNoTests --verbose -c jest-integration.config.js --runInBand",
    "test:integration:watch": "yarn test:integration --watch",
    "test:i": "yarn test:integration",
    "test:i:w": "yarn test:integration:watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test:coverage && yarn test:i",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^3.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.9.0",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "*"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fast-glob": "^3.2.11",
    "jsonwebtoken": "^8.5.1",
    "validator": "^13.6.0"
  }
}
